# Maven Download section
MAVEN_363_URL=https://ftp.cixug.es/apache/maven/maven-3/3.6.3/binaries/apache-maven-3.6.3-bin.tar.gz
MAVEN_363_TARGET=$HOME/maven/${LONG_APP_NAME_LOWER}
