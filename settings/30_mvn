MAVEN_URL=$MAVEN_363_URL
MAVEN_TARGET=$MAVEN_363_TARGET
MAVEN_TARFILE=$MAVEN_363_TARFILE 

# End Maven Download section

# maven section
M2_365_HOME=$MAVEN_363_TARGET/apache-maven-3.6.3
M2_HOME=$M2_365_HOME
# end maven section

